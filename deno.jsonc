{
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --allow-sys --allow-write --allow-run --watch=backendjs/components,main.ts main.ts",
    "check": "deno check main.ts",
    "lint": "deno lint"
  },
  "compilerOptions": {
    "allowUnreachableCode": true,
    "strictNullChecks": false,
    "noImplicitAny": false
  },
  "lint": {
    "rules": {
      "include": [],
      "exclude": ["no-explicit-any", "no-empty", "no-window", "no-window-prefix", "no-unused-vars", "no-unreachable"]
    }
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 125,
    "semiColons": true,
    "singleQuote": true,
    "useTabs": false
  },
  "imports": {
  }
}
